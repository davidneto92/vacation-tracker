<%= form_tag("/saved_trips", method: :post) do %> 
  <%= hidden_field_tag(:start_point, @start_point_json) %>
  <%= hidden_field_tag(:destination_name, @destination_string) %>
  <%= hidden_field_tag(:destination_id, @destination_id) %>
  <%= hidden_field_tag(:found_parks_list, @found_parks_list) %>
  <%= hidden_field_tag(:directions_data, @directions_data) %>

  <%= hidden_field_tag(:trip_type, @trip_type) %>
  
  <%= submit_tag("Save this trip", class: "button") %>
<% end %>